<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Loop Dashboard</title>
    <link rel="stylesheet" type="text/css" href="styles/main.css" />
  </head>
  <body>
    <div class="global--dashboard">
      <nav class="navigation--side">
        <a class="navigation__logo" href="front-dashboard.html"> </a>
        <div class="navigation__container">
          <a
            class="navigation__button navigation__button--active"
            href="front-dashboard.html"
          >
            <span class="navigation__icon navigation__icon--dashboard"> </span>
            <p class="navigation__text navigation__text--active">
              Dashboard
            </p>
          </a>
          <a class="navigation__button" href="#">
            <span class="navigation__icon navigation__icon--calendar"></span>
            <p class="navigation__text">Calendar</p>
          </a>
          <a class="navigation__button" href="courses.html">
            <span class="navigation__icon navigation__icon--courses"></span>
            <p class="navigation__text">Courses</p>
          </a>
          <a class="navigation__button" href="#">
            <span class="navigation__icon navigation__icon--clock"></span>
            <p class="navigation__text">Clock</p>
          </a>
        </div>
        <a class="navigation__button" href="#">
          <span class="navigation__icon navigation__icon--settings"></span>
          <p class="navigation__text">Settings</p>
        </a>
      </nav>
      <header class="header">
        <h2 class="header__text" id="header__text">Dashboard</h2>
        <div class="header__profile">
          <a class="header__button">
            <span class="header__icon header__icon--notifications"></span>
          </a>
          <a class="header__button">
            <span class="header__icon header__icon--profile"></span>
          </a>
        </div>
      </header>
      <main class="dashboard">
        <div class="greeting">
          <div class="greeting_text">
            <h4 class="greeting__headline">Welcome Back, Person.</h4>
            <p class="greeting__subtitle" id="greeting__quote"></p>
            <p
              class="greeting__subtitle--alternative"
              id="greeting__author"
            ></p>
          </div>
          <img class="greeting__image" src="assets/svg/books.svg" alt="books" />
        </div>
        <div class="calendar" id="calendar">
          <h5 class="calendar__headline" id="calendar__headline"></h5>
          <button class="calendar__button" id="calendar__buttonPrevious">
            <img
              src="assets/svg/arrow-left.svg"
              alt="left arrow"
              class="calendar__icon"
            />
          </button>
          <button class="calendar__button" id="calendar__buttonNext">
            <img
              src="assets/svg/arrow-right.svg"
              alt="right arrow"
              class="calendar__icon"
            />
          </button>
          <h6 class="calendar__weekdays">Sun</h6>
          <h6 class="calendar__weekdays">Mon</h6>
          <h6 class="calendar__weekdays">Tue</h6>
          <h6 class="calendar__weekdays">Wed</h6>
          <h6 class="calendar__weekdays">Thu</h6>
          <h6 class="calendar__weekdays">Fri</h6>
          <h6 class="calendar__weekdays">Sat</h6>
        </div>
        <div class="alarms">
          <h5 class="alarms__headline">Your Alarms and Timers</h5>
          <div class="alarms__container">
            <div class="alarms__time">
              <div class="clock">
                <div class="clock__container clock__container--hour">
                  <div id="clock__hour" class="clock__hour"></div>
                </div>
                <div class="clock__container clock__container--minute">
                  <div id="clock__minute" class="clock__minute"></div>
                </div>
                <div class="clock__container clock__container--second">
                  <div id="clock__second" class="clock__second"></div>
                </div>
              </div>
              <h6 id="alarms__digital" class="alarms__digital"></h6>
            </div>
            <div class="alarms__grid">
              <div class="alarms__module">
                <div class="alarms__container--icon alarms__container--green">
                  <img
                    src="assets/svg/clock--white.svg"
                    alt="clock"
                    class="alarms__icon"
                  />
                </div>
                <div class="alarms__text">
                  <h6 class="alarms__timer">00:00:00</h6>
                  <p class="alarms__title">End of Course Title</p>
                </div>
              </div>
              <div class="alarms__module">
                <div class="alarms__container--icon alarms__container--cyan">
                  <img
                    src="assets/svg/clock--white.svg"
                    alt="clock"
                    class="alarms__icon"
                  />
                </div>
                <div class="alarms__text">
                  <h6 class="alarms__timer">00:00:00</h6>
                  <p class="alarms__title">Timer Title</p>
                </div>
              </div>
              <div class="alarms__module">
                <div class="alarms__container--icon alarms__container--blue">
                  <img
                    src="assets/svg/clock--white.svg"
                    alt="clock"
                    class="alarms__icon"
                  />
                </div>
                <div class="alarms__text">
                  <h6 class="alarms__timer">12:00 AM</h6>
                  <p class="alarms__title">Alarm Title</p>
                </div>
              </div>
            </div>
          </div>
        </div>
        <h5 class="course__headline">Your Courses</h5>
        <div class="course__container course__container--first">
          <div class="course__container--icon course__container--green">
            <img
              src="assets/svg/folder.svg"
              alt="folder"
              class="course__icon"
            />
          </div>
          <div class="course__text">
            <h6 class="course__title">Course Title</h6>
            <p class="course__subtitle">Day, 00:00 AM - 00:00 PM</p>
          </div>
        </div>
        <div class="course__container course__container--second">
          <div class="course__container--icon course__container--cyan">
            <img
              src="assets/svg/folder.svg"
              alt="folder"
              class="course__icon"
            />
          </div>
          <div class="course__text">
            <h6 class="course__title">Course Title</h6>
            <p class="course__subtitle">Day, 00:00 AM - 00:00 PM</p>
          </div>
        </div>
        <div class="course__container course__container--third">
          <div class="course__container--icon course__container--blue">
            <img
              src="assets/svg/folder.svg"
              alt="folder"
              class="course__icon"
            />
          </div>
          <div class="course__text">
            <h6 class="course__title">Course Title</h6>
            <p class="course__subtitle">Day, 00:00 AM - 00:00 PM</p>
          </div>
        </div>
        <div class="tasks">
          <h5 class="tasks__headline">Your Events and Tasks</h5>
          <div class="tasks__grid">
            <div class="tasks__container">
              <div class="tasks__container--icon tasks__container--green">
                <img
                  src="assets/svg/event.svg"
                  alt="folder"
                  class="tasks__icon"
                />
              </div>
              <div class="tasks__text">
                <h6 class="tasks__title">Event Title</h6>
                <p class="tasks__subtitle">Day, 00:00 AM - 00:00 PM</p>
              </div>
            </div>
            <div class="tasks__container">
              <div class="tasks__container--icon tasks__container--cyan">
                <img
                  src="assets/svg/event.svg"
                  alt="folder"
                  class="tasks__icon"
                />
              </div>
              <div class="tasks__text">
                <h6 class="tasks__title">Event Title</h6>
                <p class="tasks__subtitle">Day, 00:00 AM - 00:00 PM</p>
              </div>
            </div>
            <div class="tasks__container">
              <div class="tasks__container--icon tasks__container--blue">
                <img
                  src="assets/svg/event.svg"
                  alt="folder"
                  class="tasks__icon"
                />
              </div>
              <div class="tasks__text">
                <h6 class="tasks__title">Event Title</h6>
                <p class="tasks__subtitle">Day, 00:00 AM - 00:00 PM</p>
              </div>
            </div>
            <div class="tasks__container">
              <div class="tasks__container--icon tasks__container--violet">
                <img
                  src="assets/svg/event.svg"
                  alt="folder"
                  class="tasks__icon"
                />
              </div>
              <div class="tasks__text">
                <h6 class="tasks__title">Event Title</h6>
                <p class="tasks__subtitle">Day, 00:00 AM - 00:00 PM</p>
              </div>
            </div>
          </div>
        </div>
      </main>
    </div>
    <script type="module" src="scripts/index.js"></script>
  </body>
</html>
